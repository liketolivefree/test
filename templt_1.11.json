{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "experimental": {
    "cache_file":{"enabled":true,"store_fakeip":false,"store_rdrc":false},
    "clash_api":{
      "external_controller": "127.0.0.1:9090",
      "default_mode":"rule",
      "external_ui": "dashboard",
      "external_ui_download_url": "https://gh-proxy.com/https://github.com/MetaCubeX/Yacd-meta/archive/gh-pages.zip",
      "external_ui_download_detour": "direct",
      "access_control_allow_origin":["http://127.0.0.1","https://yacd.metacubex.one"],
      "access_control_allow_private_network": true
    }
  },
  "dns": {
    "servers": [
      {"tag":"local-dns","address":"local","strategy":"prefer_ipv4"},
      {"tag":"direct-dns","address":"8.8.8.8","strategy":"prefer_ipv4","detour":"direct","server_port":53},      
      {"tag":"remote-dns","address":"https://1.1.1.1/dns-query","strategy":"prefer_ipv4","detour":"qqoli","server_port":443},
      {"tag":"backup-dns","address":"https://94.140.14.14/dns-query","strategy":"prefer_ipv4","detour":"qqoli","server_port":443}
    ],
    "rules": [
      {"outbound":"any","server":"direct-dns","disable_cache":false},
      {"domain_suffix":[".xiaomi.com",".xiaomi.net",".miui.com",".mi.com"],"action":"reject","method":"default","no_drop":false},
      {"ip_cidr":["0.0.0.0/32","10.10.34.0/24","240.0.0.0/4"],"action":"reject","method":"default","no_drop":false},
      {"rule_set":"aff","action":"reject","method":"default","no_drop":false},
      {"clash_mode":"Direct","action":"route","server":"direct-dns","strategy":"prefer_ipv4"},
      {"clash_mode":"Global","action":"route","server":"backup-dns","strategy":"prefer_ipv4"},
      {"domain_suffix":".ir","action":"route","server":"direct-dns","strategy":"prefer_ipv4"},
      {"rule_set":"xal","action":"route","server":"direct-dns","strategy":"prefer_ipv4"},
      {"rule_set":"oki","action":"route","server":"remote-dns","strategy":"prefer_ipv4"}
    ],
    "independent_cache": true,
    "disable_cache": false,
    "disable_expire": false,
    "final": "remote-dns"
  },
  "ntp": {
    "enabled": true,
    "server": "pool.ntp.org",
    "server_port": 123,
    "interval": "30m",
    "detour": "direct"
  },
  "inbounds": [
    {
      "tag": "tun-in",
      "type": "tun",
      "address": "172.19.0.1/30",
      "interface_name": "tun0",
      "mtu": 9000,
      "auto_route": true,
      "strict_route": true,
      "endpoint_independent_nat": true,
      "stack": "mixed",
      "platform": {"http_proxy":{"enabled":true,"server":"127.0.0.1","server_port":7890}}
    },
    {
      "tag": "mixed-in",
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 7891
    }
  ],
  "outbounds": [
    {
      "tag":"qqoli",
      "type":"selector",
      "interrupt_exist_connections": false,
      "outbounds":["mmobi","{all}"]
    },
   {
      "tag":"mmobi",
      "type":"urltest",
      "url": "https://www.gstatic.com/generate_204",
      "interrupt_exist_connections": false,
      "interval": "5m",
      "tolerance": 50,
      "outbounds":["{all}"]
    },
    {
      "tag": "direct",
      "type": "direct"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "override_android_vpn": true,
    "final": "qqoli",
    "rules": [
      {"action":"sniff","sniffer": ["http","tls","dns"],"timeout":"1s"},
      {"action": "resolve","strategy":"prefer_ipv4"},
      {"inbound":"tun-in","action":"sniff","sniffer": ["http","tls","dns"],"timeout":"1s"},
      {"inbound":"mixed-in","action":"sniff","sniffer": ["http","tls","dns"],"timeout":"1s"},
      {"inbound":"tun-in","action": "resolve","strategy":"prefer_ipv4"},
      {"inbound":"mixed-in","action": "resolve","strategy":"prefer_ipv4"},
      {"type":"logical","mode":"or","rules":[{"protocol":"dns"},{"port":53}],"action":"hijack-dns"},
      {"protocol":"quic","action":"reject","method":"default","no_drop":false},
      {"type":"logical","mode":"and","rules":[{"network":"tcp"},{"port":853}],"action":"reject","method":"default","no_drop":false},
      {"type":"logical","mode":"and","rules":[{"network":"udp"},{"port":443}],"action":"reject","method":"default","no_drop":false},
      {"domain_suffix":[".xiaomi.com",".xiaomi.net",".miui.com",".mi.com"],"action":"reject","method":"default","no_drop":false},
      {"ip_cidr":["0.0.0.0/32","10.10.34.0/24","240.0.0.0/4"],"action":"reject","method":"default","no_drop":false},
      {"rule_set":"aff","action":"reject","method":"default","no_drop":false},
      {"ip_is_private": true,"outbound": "direct"},
      {"domain_suffix":".ir","outbound":"direct"},
      {"clash_mode":"Direct","outbound":"direct"},
      {"rule_set":"oki","outbound":"direct"},
      {"rule_set":"xal","outbound":"qqoli"},
      {"clash_mode":"Global","outbound":"qqoli"}
    ],
    "rule_set": [
      {
        "tag":"aff",
        "type":"remote",
        "format":"binary",
        "url":"https://raw.githubusercontent.com/liketolivefree/test/main/aff.json",
        "download_detour":"direct"
      },
      {
        "tag":"oki",
        "type":"remote",
        "format":"binary",
        "url":"https://raw.githubusercontent.com/liketolivefree/test/main/oki.json",
        "download_detour":"direct"
      },
      {
        "tag":"xal",
        "type":"remote",
        "format":"binary",
        "url":"https://raw.githubusercontent.com/liketolivefree/test/main/xal.json",
        "download_detour":"direct"
      }
    ]
  }
}
